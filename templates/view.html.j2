{% extends "base.html.j2" %}
{% block content %}
{## h-100 causes problems... ##}
<div class="row">
  <div class="col-12 col-lg-9">
    <!-- Nav tabs -->
    <ul class="nav nav-tabs d-none d-sm-flex" id="myTab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="graph-tab" data-toggle="tab" href="#graph" role="tab" aria-controls="graph" aria-selected="true">Graph</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="list-tab" data-toggle="tab" href="#list" role="tab" aria-controls="list" aria-selected="false">List</a>
      </li>
    </ul>
    <!-- Tab panes -->
    <div class="tab-content h-100" id="myTabContent">
      <div class="tab-pane tab-height active" id="graph" role="tabpanel" aria-labelledby="graph-tab">
        {% include 'graph.html.j2' %}
      </div>
      <div class="tab-pane tab-height overflow-auto" id="list" role="tabpanel" aria-labelledby="list-tab">
        {% include "list.html.j2" %}  
      </div>
    </div>
  </div>
  <div class="col-12 col-lg-3">
    {% include 'side.html.j2' %}
  </div>
</div>
{% endblock %}